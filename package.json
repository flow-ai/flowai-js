{
  "name": "flowai-js",
  "description": "The flow.ai Javascript SDK",
  "main": "./lib/index.js",
  "version": "1.6.9",
  "author": "Gijs van de Nieuwegiessen",
  "contributors": [
    "Maksym Domariev <max@flow.ai>",
    "Gijs van de Nieuwegiessen <gijs@flow.ai>",
    "Murat Ã–zmerd <murat@flow.ai>"
  ],
  "repository": "https://github.com/flow-ai/flowai-js",
  "homepage": "https://flow.ai",
  "dependencies": {
    "backo": "^1.1.0",
    "debug": "^4.1.0",
    "events": "^3.0.0",
    "fetch-everywhere": "^1.0.5",
    "form-data": "^2.3.3",
    "node-localstorage": "^1.3.1",
    "querystring": "^0.2.0",
    "uuid-v4": "^0.1.0",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chai-eventemitter": "^1.1.1",
    "dmd-clear": "^0.1.2",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc-strip-async-await": "^0.1.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "sinon": "^7.1.0",
    "uglify-js": "^3.4.9",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "preexample": "npm run build",
    "example": "DEBUG=flowai:* node ./example/node",
    "prebuild": "npm run docs",
    "build:dist": "BABEL_ENV=dist NODE_ENV=production webpack --display-error-details --config ./webpack.config.js",
    "build:lib": "BABEL_ENV=lib NODE_ENV=production babel ./src --out-dir ./lib",
    "build": "npm run build:dist && npm run build:lib",
    "test": "BABEL_ENV=lib mocha --recursive --reporter spec --compilers js:@babel/register --require @babel/polyfill --watch",
    "eslint": "eslint ./src --fix",
    "prepublish": "npm run build",
    "docs": "jsdoc2md -c .jsdoc ./src/**/*.js --plugin dmd-clear --template ./src/README.hbs > README.md",
    "docs:reference": "jsdoc2md -c .jsdoc ./src/**/*.js --plugin dmd-clear --heading-depth=2 > REFERENCE.md"
  }
}
